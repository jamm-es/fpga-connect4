module vga_tb(hSync, vSync);

    output hSync;
    output vSync;
    
    reg clk;
    initial begin
        forever begin
            clk = 0;
            #5
            clk = ~clk;
            #5
        end
    end

    vga display(.sysClk(clk), .hSync(hSync), .vSync(vSync));

endmodule
